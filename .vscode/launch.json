        },
        {
            "name": "Flutter (TECNO KF8)",
            "request": "launch",
            "type": "dart",
            "deviceId": "TECNO KF8",
            "flutterMode": "debug",
            "cwd": "${workspaceFolder}",
            "args": [
                "--no-sound-null-safety"
